{
  "expo": {
    "name": "CLT Lifting Club",
    "slug": "cltliftingclub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "cltliftingclub",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/IMG_1936.jpg",
 "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "newArchEnabled": true,
    "android": {
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.adickinson.CltLiftingClub"
    },
    "ios": {
      "buildNumber": "6",
      "supportsTablet": true,
      "bundleIdentifier": "com.adickinson.CltLiftingClub",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow access to your photo library to upload a profile picture.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "07d8099c-e70f-42cc-8dc5-32df77a6dee9"
      }
    },
    "owner": "anndreuis"
  }
}
